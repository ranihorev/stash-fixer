{
  "name": "StashFixer",
  "version": "0.0.2",
  "manifest_version": 3,
  "description": "Replaces the commit message with the title of the PR",
  "homepage_url": "https://github.com/ranihorev/stash-fixer",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://stash.corp.netflix.com/*/pull-requests/*"
      ],
      "js": [
        "js/content.js"
      ]
    }
  ]
}